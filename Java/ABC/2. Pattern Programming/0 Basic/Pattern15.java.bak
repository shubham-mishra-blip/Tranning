class Pattern15 
{
	public static void main(String[] args) 
	{
		for(int i = 0; i <= 4; i++){
			for(int k = 4; k >= i; k --){
				System.out.print(" ");
			}
			for(int j = 1; j <= (2*i)+1; j ++){
				System.out.print("*");
		}
		System.out.println();
	}

	for(int i = 0; i <= 4; i++){
				System.out.print(" ");
			
			for(int j = 0; j <= 8; j ++){
				if(j==3 || j==4){
					System.out.print(" ");
				}else
				System.out.print("*");
		}
		System.out.println();
	}
	
}
}
